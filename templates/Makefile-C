CC = gcc
SRCS = $(wildcard src/*.c src/*.h)
OBJS = $(SRCS:.c=.o)
DELOBJS = $(OBJS:.h=.o)
CFLAGS = .$CFLAGS
LDFLAGS = .$LDFLAGS 

all: .$PROJNAME

clean:
	rm -f build/.$PROJNAME $(DELOBJS)

.$PROJNAME: $(OBJS)
	$(CC) $(OBJS) -o build/.$PROJNAME $(CFLAGS) 

%.o: %.c
	@$(CC) $(CFLAGS) -c $< -o $@
